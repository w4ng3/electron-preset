  "dependencies": {
    "vue-i18n": "^11.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@iconify-json/carbon": "^1.2.8",
    "@types/node": "^22.13.4",
    "@unocss/eslint-plugin": "66.1.0-beta.6",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "simple-git-hooks": "^2.12.1",
    "typescript": "catalog:",
    "unocss": "catalog:"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": ">=9.0.0",
        "vite": ">=6.0.0"
      }
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
