  "keywords": [],
  "engines": {
    "node": ">=18.12",
    "pnpm": ">=9.5"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "client:dev": "pnpm -F client dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:view": "npx @eslint/config-inspector",
